package threadclasses;

public class bike  extends Thread {
	public void run(){
		System.out.println("bike is running.....");
		for(int i=1;i<=10;i++)
			System.out.println(i);
		try {
			Thread.sleep(20000);
		} catch (InterruptedException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
	}

}
